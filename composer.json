{
    "name": "rasumo/sms",
    "description": "Laravel SMS notification channel that makes is easy to change providers",
    "homepage": "https://github.com/rasumo/sms",
    "license": "MIT",
    "authors": [
        {
            "name": "Marvin Whitfield",
            "email": "marvin.whitfield@gmail.com",
            "homepage": "http://www.marvinwhitfield.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "6.* || 7.*",
        "illuminate/notifications": "^5.5 || ^6.0 || ^7.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Rasumo\\Sms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rasumo\\Sms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rasumo\\Sms\\SmsServiceProvider"
            ]
        }
    }
}
